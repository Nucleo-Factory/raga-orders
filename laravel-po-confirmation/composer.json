{
    "name": "raga-orders/laravel-po-confirmation",
    "description": "Módulo de confirmación de órdenes de compra via email para Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Raga Orders",
            "email": "info@ragaorders.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "RagaOrders\\POConfirmation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RagaOrders\\POConfirmation\\POConfirmationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
